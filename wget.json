{
    "Name"     : "wget",
    "Version"  : "1.13",
    "Url"      : "http://mirrors.kernel.org/gnu/wget/wget-1.13.tar.gz",
    "StageDir" : "",
    "Flags"    : [
               "--without-ssl"
    ],
    "Date"  : "0001-01-01T00:00:00Z",
    "Build" : [
            "make distclean",
            "$SRCDIR/configure $Flags",
            "make"
    ],
    "Package" : [
              "make install DESTDIR=$PKGDIR"
    ],
    "Depends" : [
              "glibc"
    ],
    "Files" : [
            "usr/bin/wget",
            "usr/etc/wgetrc",
            "usr/share/info/dir",
            "usr/share/info/wget.info",
            "usr/share/man/man1/wget.1"
    ]
}
