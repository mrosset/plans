{
    "Identity"    : "test user \u003ctest@test.com\u003e",
    "Arch"        : "x86_64",
    "OS"          : "linux",
    "Root"        : "/",
    "PlansRepo"   : "http://bitbucket.org/strings/plans",
    "Cache"       : "$HOME/via/cache",
    "DB"          : "home/mrosset/install/db",
    "Plans"       : "$HOME/via/plans",
    "Repo"        : "$HOME/via/repo",
    "Binary"      : "http://jupiter:8080/repo",
    "Flags"       : [
                  "--config-cache",
                  "--disable-acl",
                  "--disable-manpages",
                  "--disable-multilib",
                  "--disable-nls",
                  "--disable-werror",
                  "--libexec=$PREFIX/lib",
                  "--prefix=$PREFIX",
                  "--sysconfdir=$PREFIX/etc",
                  "--without-manpages",
                  "-q"
    ],            
    "Env"         : {
                  "CFLAGS"       : "-pipe -O2 -mtune=generic",
                  "CONFIG_SHELL" : "/bin/sh",
                  "CXXFLAGS"     : "-pipe -O2 -mtune=generic",
                  "LDFLAGS"      : "-Wl,-rpath -Wl,/home/mrosset/install/lib -Wl,--as-needed -Wl,--dynamic-linker,/home/mrosset/install/lib/ld-linux-x86-64.so.2",
                  "LIBRARY_PATH" : "/home/mrosset/install/lib",
                  "MAKEFLAGS"    : "-sw -j4",
                  "MAKEINFO"     : "missing",
                  "PATH"         : "$GOPATH/bin:/home/mrosset/install/lib/ccache:$HOME/bin:/home/mrosset/install/bin:/bin:/usr/bin",
                  "PREFIX"       : "$HOME/install"
    },            
    "Remove"      : [
                  "home/mrosset/install/share/doc",
                  "home/mrosset/install/share/info",
                  "home/mrosset/install/share/man"
    ],            
    "PostInstall" : null
}
