{
    "Branch"    : "x86_64-linux-android27",
    "Identity"  : "test user \u003ctest@test.com\u003e",
    "Arch"      : "aarch64",
    "OS"        : "android",
    "Root"      : "/",
    "PlansRepo" : "http://github.com/mrosset/plans",
    "Threads"   : 8,
    "IpfsApi"   : "localhost:5001",
    "Cache"     : "$HOME/.cache/via",
    "DB"        : "/data/data/org.golang.todo.libviaexample/files/via/db",
    "Plans"     : "$GOPATH/src/github.com/mrosset/via/plans",
    "Repo"      : "$GOPATH/src/github.com/mrosset/via/publish/repo",
    "Binary"    : "http://localhost:8080/ipfs/",
    "Prefix"    : "/data/data/org.golang.todo.libviaexample/files/via",
    "Flags"     : [
                "--build=x86_64-linux-gnu",
                "--config-cache",
                "--disable-multilib",
                "--disable-nls",
                "--enable-shared",
                "--host={{.Branch}}",
                "--libexec={{.Prefix}}/lib",
                "--prefix=$PREFIX",
                "--sbindir={{.Prefix}}/bin",
                "--sysconfdir={{.Prefix}}/etc",
                "-q"
    ],
    "Env" : {
          "CC"              : "{{.Branch}}-clang",
          "CFLAGS"          : "-I{{.Prefix}}/include -pipe -O2 -mtune=generic",
          "CONFIG_SITE"     : "",
          "CXXFLAGS"        : "$CFLAGS",
          "LDFLAGS"         : "-L{{.Prefix}}/lib -Wl,-rpath -Wl,$HOME/local/lib:$PREFIX/lib",
          "LIBRARY_PATH"    : "$PREFIX/lib",
          "LIBTOOLFLAGS"    : "--silent",
          "MAKEFLAGS"       : "-sw -j8",
          "PATH"            : "$HOME/arm/bin:$GOPATH/bin:$PREFIX/lib/ccache:$HOME/bin:/bin:/usr/bin",
          "PKG_CONFIG_PATH" : "$PREFIX/lib/pkgconfig",
          "PREFIX"          : "{{.Prefix}}",
          "STRIP"           : "{{.Branch}}-strip"
    },
    "Remove" : [
             "$PREFIX/share/doc",
             "$PREFIX/share/info",
             "$PREFIX/share/man"
    ],
    "PostInstall" : null
}
