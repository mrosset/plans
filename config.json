{
    "Identity"  : "test user \u003ctest@test.com\u003e",
    "Arch"      : "x86_64",
    "OS"        : "darwin16.0.0",
    "Root"      : "/",
    "PlansRepo" : "http://github.com/mrosset/plans",
    "Cache"     : "$HOME/via_cache",
    "DB"        : "/usr/local/via/db",
    "Plans"     : "$GOPATH/src/github.com/mrosset/via/plans",
    "Repo"      : "$GOPATH/src/github.com/mrosset/via/publish/repo",
    "Binary"    : "https://github.com/mrosset/publish/raw/master/repo/darwin16.0.0",
    "Flags"     : [
                "--config-cache",
                "--disable-man",
                "--disable-multilib",
                "--disable-nls",
                "--disable-werror",
                "--enable-shared",
                "--libexec=$PREFIX/lib",
                "--prefix=$PREFIX",
                "--sysconfdir=$PREFIX/etc",
                "--without-manpages",
                "-q"
    ],
    "Env" : {
          "CFLAGS"              : "-pipe -O2 -mtune=generic -Qunused-arguments",
          "CHOST"               : "x86_64-apple-darwin16.0.0",
          "CXXFLAGS"            : "$CFLAGS",
          "LIBRARY_PATH"        : "$PREFIX/lib",
          "MAKEFLAGS"           : "-sw -j7",
          "MAKEINFO"            : "missing",
          "OCPLUS_INCLUDE_PATH" : "$PREFIX/include",
          "OC_INCLUDE_PATH"     : "$PREFIX/include",
          "OLDFLAGS"            : "-Wl,-rpath -Wl,$PREFIX/lib",
          "PATH"                : "$GOPATH/bin:$PREFIX/lib/ccache:$HOME/bin:$PREFIX/bin:/bin:/usr/bin",
          "PKG_CONFIG_PATH"     : "$PREFIX/lib/pkgconfig",
          "PREFIX"              : "/usr/local/via"
    },
    "Remove" : [
             "$PREFIX/share/doc",
             "$PREFIX/share/info",
             "$PREFIX/share/man"
    ],
    "PostInstall" : null
}
