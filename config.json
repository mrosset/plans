{
    "Identity"  : "test user \u003ctest@test.com\u003e",
    "Arch"      : "x86_64",
    "OS"        : "linux",
    "Root"      : "/",
    "PlansRepo" : "http://jupiter/plans",
    "Cache"     : "$HOME/via/cache",
    "DB"        : "usr/local/via/db",
    "Plans"     : "$HOME/via/plans",
    "Repo"      : "$HOME/via/repo",
    "Flags"     : [
                "--config-cache",
                "--disable-acl",
                "--disable-manpages",
                "--disable-multilib",
                "--disable-nls",
                "--disable-werror",
                "--libexec=$PREFIX/lib",
                "--prefix=$PREFIX",
                "--sysconfdir=$PREFIX/etc",
                "--without-manpages"
    ],          
    "Env"       : {
                "CFLAGS"   : "-pipe -O2 -mtune=generic",
                "CXXFLAGS" : "-pipe -O2 -mtune=generic",
                "LDFLAGS"  : "-Wl,--as-needed -Wl,--dynamic-linker,/usr/local/via/lib/ld-linux-x86-64.so.2",
                "PATH"     : "$HOME/bin:/usr/local/via/bin:$HOME/tools/bin:/bin:/usr/bin",
                "PREFIX"   : "/usr/local/via"
    },          
    "Remove"    : [
                "usr/local/via/share/doc",
                "usr/local/via/share/info",
                "usr/local/via/share/man"
    ]
}
