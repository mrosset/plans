{
    "Branch"    : "extras",
    "Identity"  : "test user \u003ctest@test.com\u003e",
    "Arch"      : "x86_64",
    "OS"        : "linux",
    "Root"      : "/",
    "PlansRepo" : "http://github.com/mrosset/plans",
    "Linker"    : "/tmp/via/lib/linker",
    "Cache"     : "$HOME/.cache/via",
    "DB"        : "/via/var/db",
    "Plans"     : "$GOPATH/src/github.com/mrosset/via/plans",
    "Repo"      : "$GOPATH/src/github.com/mrosset/via/publish",
    "Binary"    : "https://gitlab.com/mike.rosset/publish/raw/{{.Branch}}/repo",
    "Prefix"    : "/via",
    "Flags"     : [
                "--build=x86_64-pc-msys",
                "--config-cache",
                "--disable-man",
                "--disable-multilib",
                "--disable-nls",
                "--disable-static",
                "--disable-werror",
                "--enable-shared",
                "--libexec={{.Prefix}}/lib",
                "--prefix=$PREFIX",
                "--sbindir={{.Prefix}}/bin",
                "--sysconfdir={{.Prefix}}/etc",
                "--without-manpages"
    ],
    "Env" : {
          "CFLAGS"          : "-pipe -O2 -mtune=generic -I/c/{{.Prefix}}/include",
          "CPPFLAGS"        : "${CFLAGS} -P",
          "CXXFLAGS"        : "$CFLAGS",
          "MAKEFLAGS"       : "-sw -j8",
          "PATH"            : "$GOPATH/bin;$HOME/bin;$PREFIX/bin;c:/via/bin;c:/msys64/usr/bin",
          "PKG_CONFIG_PATH" : "$PREFIX/lib/pkgconfig",
          "PREFIX"          : "{{.Prefix}}"
    },
    "Remove" : [
             "$PREFIX/share/doc",
             "$PREFIX/share/info",
             "$PREFIX/share/man"
    ],
    "PostInstall" : null
}
