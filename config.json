{
    "Identity"    : "test user \u003ctest@test.com\u003e",
    "Arch"        : "x86_64",
    "OS"          : "linux",
    "Root"        : "/",
    "PlansRepo"   : "http://bitbucket.org/strings/plans",
    "Cache"       : "$HOME/via/cache",
    "DB"          : "usr/local/via/db",
    "Plans"       : "$HOME/via/plans",
    "Repo"        : "$HOME/via/repo",
    "Binary"      : "http://jupiter:8080/repo",
    "Flags"       : [
                  "--config-cache",
                  "--disable-acl",
                  "--disable-manpages",
                  "--disable-multilib",
                  "--disable-nls",
                  "--disable-werror",
                  "--libexec=$PREFIX/lib",
                  "--prefix=$PREFIX",
                  "--sysconfdir=$PREFIX/etc",
				  "-q",
                  "--without-manpages"
    ],            
    "Env"         : {
                  "PREFIX"   : "$HOME/install/via",
                  "CFLAGS"   : "-pipe -O2 -mtune=generic",
                  "CXXFLAGS" : "-pipe -O2 -mtune=generic",
				  "MAKEFLAGS" : "-sw -j6",
                  "LDFLAGS"  : "-Wl,--as-needed -Wl,--dynamic-linker,/usr/local/via/lib/ld-linux-x86-64.so.2",
                  "PATH"     : "$GOPATH/bin:/usr/local/via/lib/ccache:$HOME/bin:/usr/local/via/bin:/bin:/usr/bin",
				  "CONFIG_SHELL" : "/bin/sh",
				  "MAKEINFO" : "missing"
    },            
    "Remove"      : [
                  "usr/local/via/share/doc",
                  "usr/local/via/share/info",
                  "usr/local/via/share/man"
    ],            
    "PostInstall" : null
}
