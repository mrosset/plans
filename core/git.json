    "Group"        : "core",
    "StageDir"     : "",
    "Inherit"      : "",
    "BuildInStage" : true,
    "Date"         : "2014-12-01T06:39:32.783441768-08:00",
    "Size"         : 24671262,
    "SubPackages"  : null,
    "Depends"      : [
                   "curl",
                   "expat",
                   "glibc",
                   "zlib"
    ],             
    "Flags"        : [
                   "--with-curl=$PREFIX",
                   "--with-zlib=$PREFIX",
                   "--without-openssl",
                   "ac_cv_fread_reads_directories=yes",
                   "ac_cv_snprintf_returns_bogus=no"
    ],             
    "Patch"        : null,
    "Build"        : [
                   "$SRCDIR/configure $Flags --with-curl=$PREFIX",
                   "make NO_INSTALL_HARDLINKS=1"
    ],             
    "Package"      : [
                   "make NO_INSTALL_HARDLINKS=1 install DESTDIR=$PKGDIR",
                   "mkdir -p $PKGDIR/$PREFIX/share/bash-completion/completions/",
                   "install -m644 ./contrib/completion/git-completion.bash $PKGDIR/$PREFIX/share/bash-completion/completions/git"
    ],             
    "PostInstall"  : null,
    "Remove"       : null,
    "Files"        : null
}
