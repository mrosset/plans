                   "cc_cv_CFLAGS__flto=no"
    ],
    "Patch" : null,
    "Build" : [
            "$SRCDIR/configure $Flags",
            "#make"
    ],
    "Package" : [
              "#fakeroot make DESTDIR=$PKGDIR install-libLTLIBRARIES install-pkgconfigdataDATA install-pkgconfigdataDATA  install-pkgconfiglibDATA  install-pkgincludeHEADERS",
              "fakeroot make install DESTDIR=$PKGDIR",
              "rm -r $PKGDIR/etc $PKGDIR/var",
              "#mkdir $PKGDIR/bin",
              "#ln -s ../usr/lib/systemd/systemd $PKGDIR/bin/systemd"
    ],
    "PostInstall" : null,
    "Remove"      : null,
    "Files"       : null
